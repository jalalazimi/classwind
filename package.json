{
  "name": "classwind",
  "type": "module",
  "version": "1.0.0",
  "author": "Jalal Azimi <jalalazimi@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/jalalazimi/classwind",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "pref": "yarn build && cd ./benchmark && node ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@types/node": "^20.10.4",
    "benchmark": "^2.1.4",
    "bumpp": "^9.2.1",
    "classcat": "^3.2.5",
    "classnames": "^2.2.6",
    "classwind": "^1.0.0",
    "clsx": "1.1.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "prettier": {
    "semi": false,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "4.0.0"
  }
}
